{
  "manifest_version": 2,

  "name": "detektIIIF",
  "description": "This extension is a starting point to create a real Chrome extension",
  "version": "0.0.55",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "background": {
    "scripts": ["app/background.js","app/manifesto.js"]
  },
  "permissions": [
    "webRequest",
    "tabs",
    "<all_urls>"
  ]
}
